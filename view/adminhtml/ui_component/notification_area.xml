        </argument>
    </columns>
    <actionsColumn name="actions" class="TNW\QuickbooksBasic\Ui\Component\Listing\Column\NotificationActions" sortOrder="20">
        <settings>
            <indexField>identity</indexField>
        </settings>
    </actionsColumn>
