<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="columns" component="TNW_QuickbooksBasic/js/grid/listing" template="TNW_QuickbooksBasic/grid/listing">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="clearAllMessage" xsi:type="string" translate="true">Disable all messages</item>
                <item name="clearMessageUrl" xsi:type="url" path="quickbooks/system/notification"/>
